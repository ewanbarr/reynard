{
    "backend":"effpaf",
    "nodes": [
    {% for node in nodes%}
    {
        "host": "{{node['host']}}",
        "port": {{node['port']}},
        "pipelines":
        [
            {
                "name":"pipeline-0",
                "pipeline_name":"PafPipeline",
                "config":
                {
                    "output_path":"/media/scratch/data/paf/filterbanks",
                    "config_file_params":
                    [
                        "IP {{node['nic0']}}, {{node['nic1']}}",
                        "GPU_ID 0, 1",
                        "SOURCE Arse",
                        "TELESCOPE_ID 8",
                        "MACHINE_DI 9",
                        "RA 00:00:00, 00:01:00",
                        "DEC 00:00:00, 00:01:00"
                    ]
                }
            }
        ]
    }
    {% if not loop.last %},{% endif %}
    {% endfor %}
    ]
}